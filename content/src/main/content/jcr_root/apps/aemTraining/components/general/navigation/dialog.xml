    jcr:primaryType="cq:Dialog"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <news
            jcr:primaryType="cq:Widget"
            title="News"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <root-path
                    jcr:primaryType="nt:unstructured"
                    fieldLabel="Root path"
                    name="./newsRootPath"
                    xtype="pathfield"/>
                <second-level-menu
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Second level menu"
                    name="./newsSubTabs"
                    xtype="multifield">
                    <fieldConfig
                        jcr:primaryType="nt:unstructured"
                        plugins="customRootPathPlugin"
                        xtype="pathfield"/>
                </second-level-menu>
            </items>
        </news>
